{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name":"client",
            "image":"9458181013/multi-client",
            "hostname":"client",
            "essential":false,
            "memory": 128
        },
        {
            "name":"server",
            "image":"9458181013/multi-server",
            "hostname":"api",
            "essential": false,
            "memory": 128
        },
        {
            "name" : "worker",
            "image" :"9458181013/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name":"ngnix",
            "image":"9458181013/multi-ngnix",
            "hostname":"ngnix",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"],
            "memory": 128
        }
    ]
}